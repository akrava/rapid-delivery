 <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/"><i class="fas fa-home"></i></a></li>
    {{#breadcrumbs}}
        {{#link}}
        <li class="breadcrumb-item"><a href="{{link}}">{{text}}</a></li>
        {{/link}}
        {{^link}}
        <li class="breadcrumb-item active" aria-current="page">{{text}}</li>
        {{/link}}
    {{/breadcrumbs}}
</ol>