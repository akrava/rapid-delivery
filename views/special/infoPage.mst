<!doctype html>
<html lang="ru">
    {{>head}}
    <body>
        <header class="navbar">
            {{> header}}
        </header>
        <main class="container mt-4" role="main">
            <nav aria-label="breadcrumb">
               {{> breadcrumb}}
            </nav>
            <h1>{{info.title}}</h1>
            <p class="mb-5">{{info.message}}</p>
        </main>
        <footer class="footer">
            {{> footer}}
        </footer>
        {{> scripts}}
    </body>
</html>